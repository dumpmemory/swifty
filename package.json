{
  "name": "Swifty",
  "version": "0.6.4",
  "description": "Modern, Lightweight, Fast and Free Password Manager",
  "repository": "https://github.com/alchaplinsky/swifty.git",
  "dependencies": {
    "@swiftyapp/cryptor": "^1.1.1",
    "classnames": "^2.2.6",
    "electron-log": "^4.3.2",
    "electron-notarize": "^1.0.0",
    "electron-unhandled": "^3.0.2",
    "electron-util": "^0.14.2",
    "fs-extra": "^9.1.0",
    "generate-password": "^1.6.0",
    "googleapis": "^67.1.1",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "mousetrap": "^1.6.5",
    "nucleon": "^1.0.19",
    "parse-domain": "^3.0.3",
    "password-validator": "^5.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "remove": "^0.1.5",
    "shortid": "^2.2.16",
    "speakeasy": "^2.0.0",
    "universal-analytics": "^0.4.23"
  },
  "scripts": {
    "lint": "npx eslint --color src",
    "start": "NODE_ENV=development bozon start",
    "test": "yarn lint && yarn test:unit && yarn test:features",
    "test:unit": "NODE_ENV=test npx jest test/units",
    "test:features": "NODE_ENV=test bozon test test/features"
  },
  "author": {
    "name": "Alex Chaplinsky",
    "email": "alchaplinsky@gmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "bozon": "^1.2.0",
    "css-loader": "^5.1.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^7.0.1",
    "electron": "^12.0.0",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "ps-node": "^0.1.6",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^11.0.1",
    "spectron": "^13.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.4"
  },
  "resolutions": {
    "node-forge": "^0.10.0",
    "js-yaml": "^3.14.1",
    "yargs-parser": "^20.2.6",
    "serialize-javascript": "^5.0.1"
  },
  "build": {
    "appId": "com.electron.swifty",
    "afterSign": "scripts/notarize.js",
    "fileAssociations": [
      {
        "ext": "swftx",
        "role": "None",
        "icon": "build/fileicon.icns",
        "mimeType": "application/vnd.swftx"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "alchaplinsky",
        "repo": "swifty"
      }
    ],
    "win": {
      "icon": "build/icon.ico",
      "publisherName": "Developer ID Application: Oleksandr Chaplinsky (UFBL3F444A)",
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ]
    },
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        "dmg",
        "zip",
        "mas"
      ]
    }
  }
}
